{
  "dockerComposeFile": "docker-compose.yml",
  "features": {
    "ghcr.io/customink/codespaces-features/common-amzn": {},
    "ghcr.io/customink/codespaces-features/docker-in-docker-amzn": {}
    // "ghcr.io/devcontainers/features/sshd:latest": {}
  },
  "remoteUser": "vscode",
  "remoteEnv": {
    "COMPOSE_HTTP_TIMEOUT": "300"
  },
  "service": "app",
  "workspaceFolder": "/workspaces/lamby_benchmark",
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/lamby_benchmark,type=bind,consistency=cached",
  "mounts": [
    "source=${localWorkspaceFolder},target=/workspaces/lamby_benchmark,type=bind,consistency=cached"
  ]
}
